{
  "name": "stickernest-server",
  "version": "2.0.0",
  "description": "StickerNest v2 Backend Server - Express + TypeScript + Prisma",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate --schema=db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=db/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=db/prisma/schema.prisma",
    "db:seed": "tsx db/seed.ts",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@prisma/client": "^5.22.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "openai": "^4.52.7",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "redis": "^5.10.0",
    "replicate": "^0.34.1",
    "sharp": "^0.34.5",
    "stripe": "^20.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.16",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "stickernest",
    "widget",
    "canvas",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "StickerNest Team",
  "license": "MIT"
}