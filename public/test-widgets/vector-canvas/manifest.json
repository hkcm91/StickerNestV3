{
  "id": "vector-canvas",
  "name": "Vector Canvas",
  "version": "1.0.0",
  "kind": "2d",
  "description": "Main canvas for vector graphics - displays and manages SVG entities",
  "author": "StickerNest",
  "entry": "index.html",
  "capabilities": {
    "draggable": true,
    "resizable": true
  },
  "icon": "data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='%231a1a2e'><rect x='3' y='3' width='18' height='18' rx='2' fill='%23fff' stroke='%231a1a2e' stroke-width='2'/><circle cx='8' cy='8' r='2' fill='%233b82f6'/><rect x='12' y='12' width='6' height='6' fill='%2322c55e'/></svg>",
  "category": "vector-graphics",
  "size": {
    "defaultWidth": 500,
    "defaultHeight": 400,
    "minWidth": 300,
    "minHeight": 200
  },
  "permissions": [],
  "events": {
    "emits": [
      "vector:entity-added",
      "vector:entity-updated",
      "vector:entity-deleted",
      "vector:selection-changed",
      "vector:entities-list",
      "vector:svg-data"
    ],
    "listens": [
      "vector:spawn-shape",
      "vector:set-fill",
      "vector:set-stroke",
      "vector:set-stroke-width",
      "vector:set-opacity",
      "vector:set-shadow",
      "vector:transform",
      "vector:delete-selected",
      "vector:duplicate-selected",
      "vector:bring-forward",
      "vector:send-backward",
      "vector:bring-to-front",
      "vector:send-to-back",
      "vector:get-entities",
      "vector:get-svg"
    ]
  }
}
