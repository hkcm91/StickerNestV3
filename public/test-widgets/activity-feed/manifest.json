{
    "id": "activity-feed",
    "name": "Activity Feed",
    "version": "1.0.0",
    "kind": "2d",
    "entry": "index.html",
    "capabilities": {
        "draggable": true,
        "resizable": true
    },
    "inputs": {
        "all-events": {
            "type": "event",
            "description": "Listens to all ecosystem events to display activity"
        }
    },
    "outputs": {
        "activity-clicked": {
            "type": "event",
            "description": "Emits when an activity item is clicked"
        }
    }
}
